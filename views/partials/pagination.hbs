<nav aria-label="Page navigation">
  <ul class="pagination  justify-content-end">
    {{#paginate pagination type="first"}}
      <li {{#if disabled}}class="disabled"{{/if}} class="page-item"><a class="page-link" href="?page={{n}}">First</a></li>
    {{/paginate}}
    {{#paginate pagination type="previous"}}
      <li {{#if disabled}}class="disabled"{{/if}} class="page-item"><a class="page-link" href="?page={{n}}">Prev</a></li>
    {{/paginate}}
    {{#paginate pagination type="middle" limit="3"}}
      <li {{#if active}}class="active"{{/if}} class="page-item"><a class="page-link" href="?page={{n}}">{{n}}</a></li>
    {{/paginate}}
    {{#paginate pagination type="next"}}
      <li {{#if disabled}}class="disabled"{{/if}} class="page-item"><a class="page-link" href="?page={{n}}">Next</a></li>
    {{/paginate}}
    {{#paginate pagination type="last"}}
      <li {{#if disabled}}class="disabled"{{/if}} class="page-item"><a class="page-link" href="?page={{n}}">Last</a></li>
    {{/paginate}}
  </ul>
  </nav>